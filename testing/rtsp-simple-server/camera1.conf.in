;*************************************************
; Configuration options specific to camera 1
;*************************************************

;*************************************************
;*****   Camera
;*************************************************
device_name MyCam1
device_id 101

;*************************************************
;*****   Source
;*************************************************
netcam_url @URL@

;*************************************************
;*****   Image
;*************************************************
width 1920
height 1080
;movie_passthrough on
framerate 30

;*************************************************
;*****   Overlays
;*************************************************
text_left CAMERA 1

;*************************************************
;*****   Movie
;*************************************************
movie_filename @RUNDIR@/CAM01_%t-%v-%Y%m%d%H%M%S
